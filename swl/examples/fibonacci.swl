begin
    let n be 0
    input n
    if n eq 0 do
        print 0
    elif n lt 0 do
        print "ERROR: n must be a positive number"
    else do
        let prev be 0
        let curr be 1
        let i be 1
        while i lt n do
            let next be prev
            add curr to next
            prev be curr
            curr be next
            add 1 to i
        end
        print curr
    end
end
